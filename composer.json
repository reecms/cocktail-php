{
    "name": "reecms/cocktail",
    "description": "The Laravel 5 asset building tool for ReeCMS",
    "keywords": ["reecms", "assets", "laravel"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Hieu Le",
            "email": "letrunghieu.cse09@gmail.com"
        }
    ],
    "require": {
        "illuminate/filesystem": "^5.1",
        "illuminate/console": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "~0.9.4",
        "orchestra/testbench": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ree\\Theme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReeTest\\Theme\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
