{
    "name": "reecms/cocktail",
    "description": "The Laravel 5 asset building tool for ReeCMS",
    "keywords": ["reecms", "assets", "laravel"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Hieu Le",
            "email": "letrunghieu.cse09@gmail.com"
        }
    ],
    "bin": ["bin/cocktail"],
    "require": {
        "illuminate/filesystem": "^5.1",
        "symfony/console": "^2.7",
        "oyejorge/less.php": "^1.7",
        "leafo/scssphp": "~0.3",
        "coffeescript/coffeescript": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "codeclimate/php-test-reporter": "dev-master",
        "mockery/mockery": "~0.9.4",
        "orchestra/testbench": "~3.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ree\\Cocktail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReeTest\\Cocktail\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
